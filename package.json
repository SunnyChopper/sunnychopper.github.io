{
  "name": "sunnysingh.tech",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "analyze": "vite-bundle-visualizer",
    "check-type-coverage": "tsx scripts/check-type-coverage.ts",
    "validate-architecture": "tsx scripts/validate-architecture.ts",
    "check-patterns": "tsx scripts/check-patterns.ts",
    "validate": "tsx scripts/validate-all.ts",
    "validate:quick": "tsx scripts/validate-all.ts --quick",
    "validate:full": "npm run validate && npm run test && npm run test:e2e",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.13.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@langchain/anthropic": "^1.3.3",
    "@langchain/core": "^1.1.8",
    "@langchain/google-genai": "^2.1.3",
    "@langchain/groq": "^1.0.2",
    "@langchain/langgraph": "^1.0.15",
    "@langchain/openai": "^1.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/prismjs": "^1.26.5",
    "aws-amplify": "^6.15.9",
    "axios": "^1.13.2",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "gh-pages": "^6.3.0",
    "katex": "^0.16.27",
    "lucide-react": "^0.562.0",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.23",
    "browserslist": "^4.28.1",
    "caniuse-lite": "^1.0.30001764",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest-axe": "^9.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
